app  HelloWorld;
hmi-version 1.0.1;




entry HelloWorldEntryButton{
	text "ConnectedDrone";
	image <rounded_white_drone_icon> ;
	onSelect open HelloWorld;
	speakable SpeakableGlobally ;
	
}

model img navigationImage ;


state HelloWorld {
	title "ConnectedDrone";
	button sayHello;
	button speechText; 
	image navigationImage[navigationImage ]{
		height 400 ;
	}

}

