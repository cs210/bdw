app  HelloWorld;
hmi-version 1.0.1;


entry HelloWorldEntryButton{
	text "Drones are awesome (helloWorldEntryButton)";
	onSelect open FlyDrone;
	image <HelloWorldAppIcon_appswitcher> ;
}



state HelloWorld {
	title "Hello World (state HelloWorld)";
	button sayHello;
}


state FlyDrone{ // --> must implement FlyDroneView
	title "click to fly drone";
	button flyDrone;
}
